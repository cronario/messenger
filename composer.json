{
    "name": "Cronario?Messenger",
    "description": "Send Message like a Boss",
    "keywords": [
      "Cronario",
      "Messenger", "Message",
      "Worker", "Job", "Result",
      "Asynchronous", "Synchronous",
      "Async", "Sync",
      "Sms", "Hipchat", "Curl", "Whatsapp", "Mail"
    ],
    "homepage": "https://stash.interkassa.com/projects/IK/repos/messenger/browse",
    "license": "MIT",
    "authors": [
        {
            "name": "Gryaznov Vladislav",
            "email": "v.gryaznov@interkassa.com",
            "homepage": "https://interkassa.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "cronario": "dev-master"
    },

    "repositories": [
    {
        "type": "package",
        "package": {
            "name": "cronario",
            "version": "dev-master",
            "source": {
                "type": "git",
                "url": "https://v.gryaznov@stash.interkassa.com/scm/ik/cronario.git",
                "reference": "master"
          }
        }
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Messenger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Messenger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1"
        }
    }
}
